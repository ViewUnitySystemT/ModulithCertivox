{
  "name": "rf-ui-portal",
  "version": "1.0.0",
  "description": "Professional RF Communication System UI Portal - All GUI/UX variants unified",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "rf-server": "python src/server/rf_backend_server.py",
    "audit-export": "python src/tools/audit_exporter.py",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "npm run build && npx @next/bundle-analyzer",
    "clean": "rm -rf .next node_modules/.cache",
    "export": "next build",
    "deploy": "npm run build && npm run export && echo. > out/.nojekyll && gh-pages -d out"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.6.0",
    "chart.js": "^4.4.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.0",
    "fuse.js": "^7.0.0",
    "lucide-react": "^0.292.0",
    "next": "^14.0.0",
    "postcss": "^8.4.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.0",
    "react-hot-toast": "^2.4.0",
    "react-use": "^17.4.0",
    "socket.io-client": "^4.7.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "zod": "^3.22.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.5.0",
    "@vitejs/plugin-react": "^4.2.0",
    "cypress": "^13.6.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.3.0",
    "jest": "^29.7.0",
    "playwright": "^1.40.0",
    "prettier": "^3.1.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "rf",
    "radio-frequency",
    "auditable",
    "communication",
    "ui-portal",
    "professional",
    "modular",
    "hardware",
    "certification"
  ],
  "author": "R.D.TEL & Gentlyoverdone",
  "license": "MIT"
}
